{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.SignalR": "Debug"
    },
    "ApplicationInsights": {
      "IncludeScopes": false,
      "LogLevel": {
        "Default": "Information"
      }
    }
  },
  "AzureDefaults": {
    "Diagnostics": {
      "IsTelemetryDisabled": false,
      "IsLoggingContentEnabled": true
    },
    "Retry": {
      "MaxRetries": 3,
      "Mode": "Exponential"
    }
  },
  "AzureMaps": {
    "SubscriptionKey": ""
  },
  "Database": {
    "DatabaseName": "doccracker",
    "IndexContainer": "indices",
    "PartitionKey": "/id"
  },
  "Search": {
    "IndexName": "km",
    "IndexerName": "km-indexer",
    "KeyField": "metadata_storage_path",
    "SuggesterName": "sg",
    "IsPathBase64Encoded": true,
    "PageSize": 4
  },
  "Storage": {
    "ContainerName": "documents",
    "JobsContainerName": "jobs",
    "SourceContainerName": "source-docs",
    "Tags": [
      {
        "Name": "documentType",
        "AllowedValues": [
          "Sitreps",
          "Briefing Notes",
          "Committee Notes",
          "Presentation"
        ]
      }
    ],
    "Metadata": [
      {
        "Name": "mission"
      },
      {
        "Name": "documentType",
        "AllowedValues": [
          "Sitreps",
          "Briefing Notes",
          "Committee Notes",
          "Presentation"
        ]
      },
      {
        "Name": "sourceType",
        "AllowedValues": [
          "Email",
          "Website"
        ]
      },
      {
        "Name": "sourcePath"
      },
      {
        "Name": "title"
      }
    ]
  },
  "EntityMap": {
    "Facets": "keyPhrases, locations"
  },
  "Customizations": {
    "Enabled": true,
    "OrganizationName": "Microsoft",
    "OrganizationLogo": "~/images/logo.png",
    "OrganizationWebSiteUrl": "https://www.microsoft.com"
  },
  "Azure": {
    "SignalR": {
      "Enabled": "true"
    }
  },
  "Assistant": {
    "BaseUri": "http://127.0.0.1:8000",
    "Enabled": false
  },
  "ConnectionStrings": {
    "KnowledgeMiningUIContext": "Server=(localdb)\\mssqllocaldb;Database=KnowledgeMining.UI.Data;Trusted_Connection=True;MultipleActiveResultSets=true"
  }
}